{
    "name": "hyperapp",
    "description": "HyperApp",
    "version": "0.0.1",
    "main": "index.js",
    "author": "Jorge Bucaran",
    "license": "MIT",
    "repository": "hyperapp/hyperapp",
    "keywords": [
        "hyperx",
        "hypertext",
        "virtual-dom"
    ],
    "scripts": {
        "build": "browserify app.js -s app -g uglifyify | uglifyjs > dist/app.min.js && browserify html.js -s html -g uglifyify | uglifyjs > dist/html.min.js",
        "gzip": "gzip -c dist/app.min.js > dist/app.min.js.gz",
        "zopfli": "zopfli dist/app.min.js -c --i1500 > dist/app.min.js.gz"
    },
    "dependencies": {
        "hyperx": "^2.0.5"
    },
    "devDependencies": {
        "browserify": "^13.1.1",
        "uglifyify": "^3.0.4",
        "uglifyjs": "^2.4.10"
    }
}
