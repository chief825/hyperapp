{
    "name": "flea",
    "description": "Tiny UI library based in Snabbdom and ES6 tagged template literals",
    "version": "0.0.17",
    "main": "index.js",
    "author": "Jorge Bucaran",
    "license": "MIT",
    "repository": "jbucaran/flea",
    "keywords": [
        "snabbdom",
        "hyperx",
        "frontend",
        "minimal",
        "functional"
    ],
    "scripts": {
        "build": "mkdir -p dist && browserify app.js -s app -g uglifyify | uglifyjs > dist/flea.min.js && browserify html.js -s html -g uglifyify | uglifyjs > dist/html.min.js",
        "gzip": "gzip -c dist/flea.min.js > dist/flea.min.js.gz",
        "zopfli": "zopfli dist/flea.min.js -c --i1500 > dist/flea.min.js.gz"
    },
    "dependencies": {
        "hyperx": "^2.0.5",
        "hyperx-to-snabbdom": "^0.2.4",
        "snabbdom": "^0.5.4"
    },
    "devDependencies": {
        "browserify": "^13.1.1",
        "uglifyify": "^3.0.4",
        "uglifyjs": "^2.4.10"
    }
}
